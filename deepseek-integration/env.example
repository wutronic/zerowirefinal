# DeepSeek API Configuration
# Copy this file to .env and set your actual values

# Required: Your DeepSeek API Key
DEEPSEEK_API_KEY=your_api_key_here

# Optional: API Configuration
DEEPSEEK_API_URL=https://api.deepseek.com
DEEPSEEK_TIMEOUT=30000
DEEPSEEK_RETRIES=3
DEEPSEEK_RETRY_DELAY=1000

# Optional: Model Selection
DEEPSEEK_CHAT_MODEL=deepseek-chat
DEEPSEEK_CODER_MODEL=deepseek-coder
DEEPSEEK_REASONING_MODEL=deepseek-reasoner

# Optional: Default Parameters
DEEPSEEK_TEMPERATURE=0.7
DEEPSEEK_MAX_TOKENS=2048
DEEPSEEK_TOP_P=0.95
DEEPSEEK_FREQ_PENALTY=0
DEEPSEEK_PRESENCE_PENALTY=0

# Optional: CLI Configuration
DEEPSEEK_VERBOSE=false
DEEPSEEK_LOG_LEVEL=info
DEEPSEEK_OUTPUT_FORMAT=text 